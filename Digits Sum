
#include<bits/stdc++.h>
#define ll long long int

using namespace std;

void solve()
{
    ll n,x = 0;
    cin >> n;
    x = n/10;
    if(n%10 == 9) x++; // for cases like 9,19,29,....
    cout << x << '\n';
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int t;
    cin >> t;

    while(t--)
    {
        solve();
    }

    return 0;
}

